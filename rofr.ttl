@prefix dct: <http://purl.org/dc/terms/> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix reg: <http://purl.org/linked-data/registry#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

<http://localhost:5000/reg/>
    a reg:Register ;
    rdfs:label "Register of Registers" ;
    rdfs:comment "This reg:Register contains the other reg:Registers of this application."@en ;
    reg:containedItemClass reg:Register ;
.

<http://localhost:5000/agent/>
    a reg:RegisteredItem ;
    rdfs:label "Register of Agents" ;
    rdfs:comment "This is a reg:Register that contains a list of Agents (foaf:Agent)."@en ;
    reg:status <http://linked.data.gov.au/def/reg-status/stable> ;
    reg:containedItemClass foaf:Agent ;
    reg:register <http://localhost:5000/reg/> ;
.

<http://localhost:5000/mediatype/>
    a reg:RegisteredItem ;
    rdfs:label "Register of Media Types" ;
    rdfs:comment "This is a reg:Register that contains a list of Media Types (dct:FileFormat)."@en ;
    reg:status <http://linked.data.gov.au/def/reg-status/stable> ;
    reg:containedItemClass dct:FileFormat ;
    reg:register <http://localhost:5000/reg/> ;
.
